{% extends 'AvanzuAdminThemeBundle:layout:base-layout.html.twig' %}

{% block javascripts %}
    {{ parent() }}
    {% for script in admin_theme.scripts %}
        <script type="application/javascript" src="{{ asset(script) }}"></script>
    {% endfor %}
    {% javascripts output='js/compiled/select2.js'
        'bundles/avanzuadmintheme/vendor/adminlte/plugins/select2/select2.full.min.js'
        'bundles/avanzuadmintheme/vendor/adminlte/plugins/select2/i18n/fr.js'
    %}
    <script type="application/javascript" src="{{ asset_url }}" charset="utf-8"></script>
    {% endjavascripts %}
    {% javascripts output='js/complied/admin.js'
        '@XdaysaysayAdminBundle/Resources/js/admin.js'
    %}
    <script type="application/javascript" src="{{ asset_url }}" charset="utf-8"></script>
    {% endjavascripts %}
    <script src="{{ asset('bundles/fosjsrouting/js/router.js') }}"></script>
    <script src="{{ path('fos_js_routing_js', { callback: 'fos.Router.setData' }) }}"></script>
{% endblock %}


{% block stylesheets %}
    {% stylesheets filter='cssrewrite' output='css/complied/select2.css'
    'bundles/avanzuadmintheme/vendor/adminlte/plugins/select2/select2.min.css'
    %}
    <link rel="stylesheet" href="{{ asset_url }}" />
    {% endstylesheets %}
    {{ parent() }}
{% endblock %}

{% block title %}
    Xdaysaysay
{% endblock %}